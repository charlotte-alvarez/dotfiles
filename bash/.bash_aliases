# ls
alias ll="ls -al"

# source (to test .bashrc changes)
alias source="source ~/.config/.bashrc && echo $?"


# Git related
alias gs="git status"
alias gc="git commit -m"
alias gp="git push"
alias gd="git diff"
alias ga="git add"
alias gaa="git add --all"
alias gap="git add -p"
alias gdc="git diff --cached"


# Nvim related
alias vim="nvim"

# Python related
alias py="python3"
alias python="python3"
